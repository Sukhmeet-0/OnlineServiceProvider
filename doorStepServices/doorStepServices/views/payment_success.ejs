   <script>
      function fetch_subcategory()
      {
         var category = localStorage.getItem("category");
         var xhttp = new XMLHttpRequest();
         var form = new FormData();
         form.append("category",category);
         xhttp.onreadystatechange = function ()
         {
            if (this.readyState == 4 && this.status == 200)
            {
               var res = this.response;
               console.log(res);

               var mainobj  = JSON.parse(res);
               console.log(mainobj);

               var arr = mainobj["ans"];
               console.log(arr);

               var ans="";

               for(var i=0; i<arr.length ; i++)
               {
                  var category = arr[i]["category"];
                  var subcategory =  arr[i]["subcategory"];
                  var description =  arr[i]["description"];
                  var photo =  arr[i]["photo"];
                  ans = ans+"<div class=\"col-md-6 col-lg-3 d-flex align-items-stretch mb-5 mb-lg-0 data-aos='fade-up'\">";
                  ans+="<div class=\"card\" style=\"width: 18rem;\">";
                  ans+=" <img src="+photo+" class=\"card-img-top\" alt=\"...\">";
                  ans+="<div class=\"card-body\">";
                  ans+="<h5 class=\"card-title\">"+subcategory+"</h5>";
                  ans+="<p class=\"card-text\">"+description+"</p>";
                  ans = ans+" <button class='btn btn-primary' onclick=\"allvenders('"+subcategory+"')\">View Vendors</button>";
                  ans+="</div>";
                  ans+="</div>";
                  ans+="</div>";
               }

               //document.getElementById("d1").innerHTML = res;
               document.getElementById("sub").innerHTML = ans;
               document.getElementById("d1").innerHTML = "<h2>"+category+"</h2>";
            }
         };

         xhttp.open("POST", "/fetch_subcategory", true);
         xhttp.send(form);
      }

      function allvenders(subcategory)
      {
//               alert(subcategory);
         localStorage.setItem("subcategory",subcategory);
         window.location.href ="/all_venders";
      }
