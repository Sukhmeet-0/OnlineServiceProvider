<script>
    function go() {
        // Send AJAX Request and get Response From Server
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function () {
            if (this.readyState == 4 && this.status == 200) {

                var res = this.responseText.trim();
                var mainobj = JSON.parse(res);
                console.log(mainobj);

                var arr = mainobj["ans"];
                console.log(arr);

                var ans = "";
                ans += "<h1>Vender Bookings</h1>";
                ans += "<table class=\"table\">";
                ans += "<tr>";
                ans += "<th>Id</th>";
                ans += "<th>photo</th>";
                ans += "<th>Date</th>";
                ans += "<th>Vender Email</th>";
                ans += "<th>User Email</th>";
                ans += "<th>Price</th>";
                ans += "<th>Payment Mode</th>";
                ans += "<th>Username</th>";
                ans += "<th>Start Slot</th>";
                ans += "<th>End Slot</th>";
                ans += "</tr>";
                ans += "<tbody>";
                for (var i = 0; i < arr.length; i++) {
                    var bookid = arr[i]["bookid"];
                    var date = arr[i]["date"];
                    var venderemail = arr[i]["venderemail"];
                    var name = arr[i]["username"];
                    var Total_price = arr[i]["Total_Price"];
                    var payment_type = arr[i]["payment_type"];
                    var username = arr[i]["username"];
                    var photo = arr[i]["photo"];
                    var start_slot = arr[i]["start_slot"];
                    var End_slot = arr[i]["end_slot"];

                    ans += "<tr>";
                    ans += "<td>" + bookid + "</td>";
                    ans += "<td><img src=" + photo + " width='50px'></td>";
                    ans += "<td>" + date + "</td>";
                    ans += "<td>" + venderemail + "</td>";
                    ans += "<td>" + name + "</td>";
                    ans += "<td>" + Total_price + "</td>";
                    ans += "<td>" + payment_type + "</td>";
                    ans += "<td>" + username + "</td>";
                    ans += "<td>" + start_slot + "</td>";
                    ans += "<td>" + End_slot + "</td>";
                    ans += "</tr>";
                }
                ans += "</tbody>";
                ans += "</table>";
                document.getElementById("ans").innerHTML = ans;
            }
        };

        xhttp.open("POST", "/venderbookings", true);
        xhttp.send();

    }
</script>
